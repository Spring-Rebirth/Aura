{
  "expo": {
    "name": "Aura",
    "slug": "aura",
    "scheme": "aura",
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/icon_app.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.mystseed.aora"
      },
      "package": "com.mystseed.aora"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "myst-seed",
    "extra": {
      "eas": {
        "projectId": "b42e4b62-07a8-4671-b223-82c614092207"
      }
    }
  }
}