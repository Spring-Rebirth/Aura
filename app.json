{
  "expo": {
    "name": "Aura",
    "slug": "aura",
    "scheme": "aura",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/icon_app.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.png",
        "backgroundColor": "#ffffff",
        "package": "com.mystseed.aura"
      },
      "package": "com.mystseed.aura"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "myst-seed",
  }
}